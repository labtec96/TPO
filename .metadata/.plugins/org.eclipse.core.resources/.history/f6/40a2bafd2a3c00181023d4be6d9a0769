package zad1;

import javax.swing.JFrame;

public class Klient extends Thread
{

	public Klient(String haslo, String kodKraju, int port)
	{
		// TODO Auto-generated constructor stub
	}

	@Override
	public void run()
	{
		MainGui ramka = new MainGui();
		ramka.setVisible(true);
		String haslo = ramka.getTfHaslo().getText();
		String kodKraju = ramka.getTfKodKraju().getText();
		String port = ramka.getTfPort().getText();
	}

}
